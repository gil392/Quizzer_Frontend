{
  "name": "final_project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:production": "npm run build && docker rmi -f quizzer_frontend && docker build . -t quizzer_frontend",
    "shutdown:production": "docker rm -f frontend",
    "start:production": "docker run --network quizzer_network -p 443:443 -p 80:80 -v ./certs/CSB.crt:/etc/ssl/certs/fullchain.pem:ro -v ./certs/myserver.key:/etc/ssl/private/privkey.pem:ro --name frontend -d quizzer_frontend",
    "restart:production": "npm run build:production && npm run shutdown:production && npm run start:production",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.1",
    "@mui/styles": "^6.4.11",
    "@react-oauth/google": "^0.12.2",
    "@reduxjs/toolkit": "^2.8.2",
    "@types/lodash": "^4.17.16",
    "axios": "^1.8.4",
    "clsx": "^2.1.1",
    "html2pdf.js": "^0.10.3",
    "lodash": "^4.17.21",
    "ramda": "^0.30.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.4.1",
    "recoil": "^0.7.7",
    "sonner": "^2.0.3",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/ramda": "^0.30.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5"
  }
}
